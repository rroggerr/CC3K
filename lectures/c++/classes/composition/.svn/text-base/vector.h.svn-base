#ifndef __VECTOR_H__
#define __VECTOR_H__

class Vector {
  int x, y, z;
 public:
  Vector(int x, int y, int z);
  Vector(const Vector &v);

  int getX() const;
  int getY() const;
  int getZ() const;

  void setX(const int newX);
  void setY(const int newY);
  void setZ(const int newZ);
};

#endif
